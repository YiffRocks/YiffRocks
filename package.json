{
  "name": "yiff.rocks",
  "version": "1.0.0",
  "description": "An image board for furries.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check-circular": "npx dpdm --tree=false --circular=true --warning=false -T .",
    "down:dev": "sudo docker-compose down --rmi 'local' -v",
    "up:dev": "sudo docker-compose up",
    "dev": "npm run down:dev && npm run up:dev",
    "recreate-db": "node --no-warnings --no-deprecation --experimental-specifier-resolution=node  --loader ts-node/esm scripts/recreate-db.ts",
    "create-users": "node --no-warnings --no-deprecation --experimental-specifier-resolution=node  --loader ts-node/esm scripts/create-users.ts",
    "create-posts": "node --no-warnings --no-deprecation --experimental-specifier-resolution=node  --loader ts-node/esm scripts/create-posts.ts",
    "recreate": "npm run recreate-db && npm run create-users && npm run create-posts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DonovanDMC/Yiff-Rocks.git"
  },
  "author": "Donovan Daniels <hewwo@yiff.rocks>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DonovanDMC/Yiff-Rocks/issues"
  },
  "homepage": "https://github.com/DonovanDMC/Yiff-Rocks#readme",
  "dependencies": {
    "aws-sdk": "^2.1116.0",
    "bcrypt": "^5.0.1",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.0.0",
    "file-type": "^17.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "gm": "^1.23.1",
    "ioredis": "^5.0.4",
    "jimp": "^0.16.1",
    "mariadb": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-fetch": "^3.2.3",
    "on-finished": "^2.4.1",
    "pg": "^8.7.3",
    "tsafe": "^0.10.0",
    "undici": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/ffprobe": "^1.1.3",
    "@types/ffprobe-static": "^2.0.1",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/gm": "^1.18.11",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.25",
    "@types/on-finished": "^2.3.1",
    "@types/pg": "^8.6.5",
    "@uwu-codes/eslint-config": "^1.0.43",
    "@uwu-codes/tsconfig": "^1.0.1",
    "dpdm": "^3.9.0",
    "e621": "^2.2.1",
    "eslint": "^8.13.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "type": "module"
}
